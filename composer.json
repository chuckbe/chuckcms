{
    "name": "chuckbe/chuckcms",
    "description": "A lightweight Laravel CMS for easy control of a website.",
    "keywords": ["chuckbe", "chuckcms", "cms", "laravel", "php"],
    "homepage": "https://github.com/chuckbe/chuckcms",
    "require": {
        "php": ">=8.0.0",
        "laravel/framework": "^9.0||^10.0",
        "laravel/ui": "^4.2",
        "barryvdh/laravel-dompdf": "^2.0.1",
        "mcamara/laravel-localization": "^1.8",
        "msurguy/honeypot": "^1.2",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-permission": "^5.8",
        "spatie/laravel-translatable": "^6.3",
        "spatie/laravel-sitemap": "^6.1",
        "unisharp/laravel-filemanager": "^2.6"
    },
    "authors": [
        {
            "name": "Karel Brijs",
            "email": "karel@chuck.be"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chuckbe\\Chuckcms\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chuckbe\\Chuckcms\\ChuckcmsServiceProvider",
                "Msurguy\\Honeypot\\HoneypotServiceProvider"
            ],
            "aliases": {
                "ChuckTemplate": "Chuckbe\\Chuckcms\\Facades\\Template",
                "Honeypot": "Msurguy\\Honeypot\\HoneypotFacade"
            }
        }
    },
    "minimum-stability": "dev"
}
